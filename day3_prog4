#include <stdio.h>  
#include <limits.h>  

int main() {
    int list_size;
    scanf("%d", &list_size); 

    if (list_size < 2) {
        printf("Array must contain at least two elements to find a second minimum.\n");
        return 1; 
    }

    int min1 = INT_MAX;
    int min2 = INT_MAX; 

    for (int i = 0; i < list_size; i++) {
        int current_element; 
        scanf("%d", &current_element); 

        if (current_element < min1) {
            min2 = min1;        
            min1 = current_element; 
        }
        else if (current_element < min2 && current_element != min1) {
            min2 = current_element; 
        }
    }
    if (min2 == INT_MAX) {
        printf("No distinct second minimum element found (e.g., all elements are identical).\n");
    } else {
        printf("%d\n", min2);
    }

    return 0; 
}
