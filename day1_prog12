#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int length, breadth, height;
    int radius;
    scanf("%d", &length);
    scanf("%d", &breadth);
    scanf("%d", &height);
    scanf("%d", &radius);
    const float PI = 3.14;
    float volume_cuboid = (float)length * breadth * height;
    float volume_cylinder = PI * radius * radius * height;
    float volume_cone = (1.0 / 3.0) * PI * radius * radius * height;
    float volume_sphere = (4.0 / 3.0) * PI * radius * radius * radius;
    printf("Volume of Cuboid:%.2f\n", volume_cuboid);
    printf("Volume of Cylinder:%.2f\n", volume_cylinder);
    printf("Volume of Cone:%.2f\n", volume_cone);
    printf("Volume of Sphere:%.2f\n", volume_sphere);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
