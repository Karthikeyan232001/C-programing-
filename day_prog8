#include <stdio.h>
#include <string.h> 
int canRotateString(char *s, char *goal) {
    int len_s = strlen(s);
    int len_goal = strlen(goal); 
    if (len_s != len_goal) {
        return 0; 
    }
    if (len_s == 0) {
        return 1; 
    }
    char temp_s[2 * len_s + 1];
    strcpy(temp_s, s);
    strcat(temp_s, s);
    if (strstr(temp_s, goal) != NULL) {
        return 1; 
    } else {
        return 0; 
    }
}

int main() {
    char s[101];    
    char goal[101];  
    scanf("%s", s);
    scanf("%s", goal);
    if (canRotateString(s, goal)) {
        printf("true\n");
    } else {
        printf("false\n");
    }

    return 0; 
}
