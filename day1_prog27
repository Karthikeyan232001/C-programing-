#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  int num1, num2;
    scanf("%d", &num1);
    scanf("%d", &num2); 
    printf("The First value before Swapping:%d\n", num1);
    printf("The Second value before Swapping:%d\n", num2);  
    num1 = num1 + num2; 
    num2 = num1 - num2; 
    num1 = num1 - num2;   
    printf("The First value after Swapping:%d\n", num1);
    printf("The Second value after Swapping:%d\n", num2);  
    return 0;
}
