#include <stdio.h>
#include <string.h> 
#include <ctype.h>  
#define MAX_CHARS 256 
int areAnagrams(char *str1, char *str2) {
    int count1[MAX_CHARS] = {0}; 
    int count2[MAX_CHARS] = {0}; 
    int i;
    int len1 = strlen(str1);
    int len2 = strlen(str2);
    if (len1 != len2) {
        return 0; 
    }
    for (i = 0; i < len1; i++) {
        if (str1[i] >= 0 && str1[i] < MAX_CHARS) {
            count1[(int)str1[i]]++;
        }
    }
    for (i = 0; i < len2; i++) {
       
        if (str2[i] >= 0 && str2[i] < MAX_CHARS) {
            count2[(int)str2[i]]++;
        }
    }
    for (i = 0; i < MAX_CHARS; i++) {
        if (count1[i] != count2[i]) {
            return 0; 
        }
    }

    return 1;
}
int main() {
    char inputString1[100]; 
    char inputString2[100]; 

    scanf("%s", inputString1);

    scanf("%s", inputString2);
    if (areAnagrams(inputString1, inputString2)) {
        printf("The given strings are an anagram\n");
    } else {
        printf("The given strings are not an anagram\n");
    }

    return 0; 
}
