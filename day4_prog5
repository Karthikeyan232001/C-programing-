#include <stdio.h>
#include <string.h> 

int main() {
    char string1[100]; 
    char string2[100]; 
    char resultString[200];
    scanf("%s", string1);
    scanf("%s", string2);
    strcpy(resultString, string1);
    strcat(resultString, string2);
    printf("%s\n", resultString);

    return 0;
}
