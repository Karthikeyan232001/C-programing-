#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int num;
    scanf("%d", &num);
    if (num % 2 == 0) {
        printf("The given input %d is Even\n", num);
    } else 
    {
        printf("The given input %d is Odd\n", num);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
