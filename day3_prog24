#include <stdio.h>

int main()
{
int n, i, j, count = 0, isDistinct;
    scanf("%d", &n);
    int arr[100];
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
  for (i = 0; i < n; i++) {
        isDistinct = 1;
        for (j = 0; j < i; j++) {
            if (arr[i] == arr[j]) {
                isDistinct = 0;
                break;
            }}
        if (isDistinct) {
            count++;
        }}
  if (count == 1)
        printf("There are 1 distinct element in the array.\n");
    else
        printf("There are %d distinct elements in the array.\n", count);
    return 0;
}
