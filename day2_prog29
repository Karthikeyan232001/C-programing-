#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   double purchase_price; 
    double repair_charges; 
    double selling_price;  
    double total_cost;
    double gain;         
    double gain_percentage;

    if (scanf("%lf", &purchase_price) != 1) {
        printf("Invalid Input\n");
        return 1;
    }
    if (scanf("%lf", &repair_charges) != 1) {
        printf("Invalid Input\n");
        return 1;
    }
    if (scanf("%lf", &selling_price) != 1) {
        printf("Invalid Input\n");
        return 1;
    }
    total_cost = purchase_price + repair_charges;
    gain = selling_price - total_cost;

  
    if (total_cost <= 0) { 
       
        printf("Cannot calculate gain percentage with zero or negative total cost.\n");
        return 1;
    }
    if (gain > 0) {
        gain_percentage = (gain / total_cost) * 100.0;
        printf("%.2f\n", gain_percentage);
    } else {
        printf("0.00\n"); // Or could print "No Gain" if explicitly required
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
