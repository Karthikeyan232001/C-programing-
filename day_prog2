#include <stdio.h>
#include <string.h>
void removeSpaces(char *str) {
    int i, j;
    for (i = 0, j = 0; str[i] != '\0'; i++) {
        if (str[i] != ' ') {
            str[j] = str[i];
            j++;
        }
    }
    str[j] = '\0'; 
}

int main() {
    char inputString[100]; 
    if (fgets(inputString, sizeof(inputString), stdin) != NULL) {
        inputString[strcspn(inputString, "\n")] = 0;
    }
    removeSpaces(inputString);
    printf("%s\n", inputString);
    return 0;
}
