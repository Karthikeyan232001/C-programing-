#include <stdio.h>
#include <string.h>
int main() {
    char word[260], ch;
    scanf("%s", word);       
    scanf(" %c", &ch);     
    int index = -1;
    int length = strlen(word);
    for (int i = 0; i < length; i++) {
        if (word[i] == ch) {
            index = i;
            break;
        }}
    if (index != -1) {
        int start = 0, end = index;
        while (start < end) {
            char temp = word[start];
            word[start] = word[end];
            word[end] = temp;
            start++;
            end--;
        }}
    printf("%s\n", word);  // Output the final result
    return 0;
}
