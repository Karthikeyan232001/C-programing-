#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int num;

    if (scanf("%d", &num) != 1) {
        printf("Invalid Input\n");
        return 1;
    }

    for (int i = 1; i <= 5; i++) {
        printf("%d * %d = %d\n", num, i, num * i);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
