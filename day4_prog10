#include <stdio.h>
#include <string.h>
#include <ctype.h>  
int main() {
    char inputString[200];  
    char camelCaseString[200]; 
    int i, j = 0;              
    int capitalizeNext = 1;  
    if (fgets(inputString, sizeof(inputString), stdin) != NULL) {
        inputString[strcspn(inputString, "\n")] = 0;
    }
    for (i = 0; inputString[i] != '\0'; i++) {
        if (inputString[i] == ' ') {
            capitalizeNext = 1;
        } else {
            if (capitalizeNext) {
                camelCaseString[j] = toupper(inputString[i]);
                capitalizeNext = 0; 
            } else {
                camelCaseString[j] = tolower(inputString[i]);
            }
            j++; 
        }
    }
    camelCaseString[j] = '\0'; 
    printf("%s\n", camelCaseString);
    return 0; 
}
